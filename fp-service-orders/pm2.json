{
  "apps": [{
    "name": "OrdersAPI",
    "script": "npm",
    // Options reference: https://pm2.io/doc/en/runtime/reference/ecosystem-file/
    "args": "start",
    "instances": 1,
    "autorestart": true,
    "watch": false,
    "max_memory_restart": "1G",
    "env": {
      "NODE_ENV": "development",
      "PORT": 10011,
      "NODE_OPTIONS": "--inspect --inspect-port=9232"
    },
    "env_production": {
      "NODE_ENV": "production",
      "PORT": 10011
    }
  }]
}
